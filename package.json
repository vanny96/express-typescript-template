    "mongoose": ">=5.7.5",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/mongoose": "^5.5.19"
  }
}
